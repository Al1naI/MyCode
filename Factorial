import java.util.Scanner;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        long number = sc.nextLong();
        long i, fact = 1;
        for(i = 1; i <= number; i++) {
            fact = fact * i;
        }
        System.out.println("изи пизи, факториал числа " + number + ": "+ fact);
    }
}
